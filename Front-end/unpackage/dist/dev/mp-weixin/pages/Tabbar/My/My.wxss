@font-face {
  font-family: 'iconfont';  /* project id 3401390 */
  src: url('#iefix') format('embedded-opentype'),
  url('https://at.alicdn.com/t/font_3401390_ydmm3na0mkr.woff2') format('woff2'),
  url('https://at.alicdn.com/t/font_3401390_ydmm3na0mkr.woff') format('woff'),
  url('https://at.alicdn.com/t/font_3401390_ydmm3na0mkr.ttf') format('truetype'),
  url('#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-yuyue:before {
  content: "\e60d";
}
.icon-wj-jzbg:before {
  content: "\e70c";
}
.icon-fankui-copy:before {
  content: "\e70d";
}
.status_bar {
  height: 25px;
  width: 100%;
  background-color: #F4F4F4;
}
.wrapper {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #F4F4F4;
}
.top {
  width: 100%;
  height: 130px;
  padding-top: 15px;
  position: relative;
}
.center {
  width: 95%;
  height: 100px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  border-radius: 5px;
}
.down {
  width: 50%;
  height: 150px;
  background: white;
  display: flex;
  flex-direction: column;
  margin-top: 10px;
  border-radius: 5px;
}
.address {
  line-height: 1;
  background-color: #fff;
  font-size: 30rpx;
  padding: 25rpx 0 25rpx 20rpx;
  margin: 10rpx 20rpx;
  color: #333;
  border-radius: 4rpx;
}
.icon-arrow {
  position: relative;
}
.icon-arrow::before {
  position: absolute;
  top: 50%;
  right: 20rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.center_top {
  display: flex;
  flex-direction: row;
  width: 80%;
  height: 80px;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid #EEEEEE;
}
.center_img {
  width: 66px;
  height: 66px;
  border-radius: 20%;
  overflow: hidden;
}
.center_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.center_img .user_head {
  width: 100%;
  height: 100%;
}
.center_info {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-left: 30px;
}
.center_name {
  font-size: 20px;
}
.email_text {
  margin-top: 15rpx;
  color: #AAAAAA;
}
.orders {
  margin: 20rpx 20rpx 0 20rpx;
  padding: 40rpx 0;
  background-color: #fff;
  border-radius: 4rpx;
}
.orders .title {
  padding-left: 20rpx;
  font-size: 30rpx;
  color: #333;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #eee;
  margin-top: -30rpx;
}
.orders .sorts .nav {
  display: flex;
}
.orders .sorts .nav .nav_item {
  width: 33%;
  text-align: center;
}
.orders .sorts .nav .nav_item view {
  width: 120rpx;
  height: 120rpx;
  background: #1058D1;
  border-radius: 60rpx;
  margin: 10px auto;
  line-height: 120rpx;
  color: #fff;
  font-size: 50rpx;
}
.orders .sorts .nav .nav_item .icon-tupian {
  font-size: 45rpx;
}
.orders .sorts .nav .nav_item text {
  font-size: 28rpx;
}
.orders [class*="icon-"] {
  flex: 1;
  font-size: 24rpx;
}
.orders [class*="icon-"]::before {
  display: block;
  font-size: 48rpx;
  margin-bottom: 8rpx;
  color: #fff;
}
.extra {
  margin: 0 20rpx;
  margin-top: 10px;
  background-color: #fff;
  border-radius: 4rpx;
}
.extra .item {
  line-height: 1;
  padding: 25rpx 0 25rpx 20rpx;
  border-bottom: 1rpx solid #eee;
  font-size: 30rpx;
  color: #333;
}
.extra button {
  text-align: left;
  background-color: #fff;
}
.extra button::after {
  border: none;
  border-radius: 0;
}
.extra2 {
  margin: 0 20rpx;
  margin-bottom: 10px;
  background-color: #fff;
  border-radius: 10rpx;
}
.list-cell {
  margin: 0 20rpx;
  margin-bottom: 10px;
  display: flex;
  align-items: baseline;
  padding: 20px;
  line-height: 30rpx;
  position: relative;
  background: #fff;
  justify-content: center;
}
.list-cell.log-out-btn {
  margin-top: 40rpx;
}
.list-cell.log-out-btn .cell-tit {
  color: #1058D1;
  text-align: center;
  margin-right: 0;
}

