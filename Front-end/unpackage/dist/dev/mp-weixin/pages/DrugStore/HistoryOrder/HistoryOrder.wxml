<view><view class="topTabBar" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block wx:for="{{orderType}}" wx:for-item="grid" wx:for-index="tbIndex" wx:key="tbIndex"><view data-event-opts="{{[['tap',[['showType',[tbIndex]]]]]}}" class="grid" bindtap="__e"><view class="{{['text',tbIndex==tabbarIndex?'on':'']}}">{{grid}}</view></view></block></view><view class="order-list"><view class="list"><block wx:if="{{list==null}}"><view class="onorder"><image src="/static/imgs/noorder.png"></image><view class="text">没有相关订单</view></view></block><block wx:for="{{list}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="type">{{typeText[row.type]}}</view><view class="order-info"><view class="left"><image src="{{row.img}}"></image></view><view class="right"><view class="name">{{''+row.name+''}}</view><view class="spec">{{row.spec}}</view><view class="price-number">￥<view class="price">{{row.price}}</view>x<view class="number">{{row.numner}}</view></view></view></view><view class="detail"><view class="number">{{"共"+row.numner+"件商品"}}</view><view class="sum">合计￥<view class="price">{{row.payment}}</view></view></view></view></block></view></view></view>